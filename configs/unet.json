{
    "network": "UNet",
    "data": {
        "N": 5,
        "test_size": 0.10,
        "data_root": "data/noised_synth_babble",
        "libri_root": "data/LibriSpeech/dev-clean",
        "batch_size": 8,
        "conv": true,
        "seed": 1,
        "srate": 16000
    },
    "model": {
        "in_shape": [256, 256],
        "in_channels": 1,
        "n_classes": 1,
        "encoder_channels": [4, 8, 16],
        "decoder_channels": [16, 8, 4],
        "retain_dim": true
    },
    "train": {
        "epochs": 10,
        "learning_rate": 0.001,
        "criterion": "BCEWithLogitsLoss"
    }
}
