{
    "network": "CDAE",
    "data": {
        "N": 10,
        "test_size": 0.10,
        "train_root": "data/BabbledLibri/train",
        "test_root": "data/BabbledLibri/test",
        "libri_root": "data/LibriSpeech/dev-clean",
        "batch_size": 8,
        "conv": true,
        "seed": 1,
        "srate": 16000
    },
    "model": {
        "in_channels": 1,
        "encoder_dims": [32, 64, 128],
        "decoder_dims": [128, 64, 32],
        "out_activation": "sigmoid",
        "batch_norm": true
    },
    "train": {
        "epochs": 2,
        "learning_rate": 0.001,
        "criterion": "MSELoss"
    }
}
