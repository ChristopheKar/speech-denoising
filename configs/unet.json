{
    "network": "UNet",
    "data": {
        "libri_path": "data/LibriSpeech/dev-clean",
        "babble_path": "data/babble.wav",
        "train_mins": 60,
        "val_mins": 10,
        "test_mins": 10,
        "srate": 16000,
        "batch_size": 32,
        "n_fft": 512,
        "hop_len": 256,
        "conv": true,
        "gains": [0.2]
    },
    "model": {
        "in_shape": [256, 64],
        "in_channels": 1,
        "n_classes": 1,
        "encoder_channels": [16, 32, 64],
        "decoder_channels": [64, 32, 16],
        "retain_dim": true
    },
    "train": {
        "epochs": 200,
        "learning_rate": 0.001,
        "criterion": "BCEWithLogitsLoss"
    }
}
